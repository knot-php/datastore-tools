{
  "name": "knot-php/datastore-tools",
  "description": "Command line tools for knot-lib/datastore",
  "keywords": ["PHP","Tools","Command line","kNot Framework"],
  "homepage": "https://github.com/knot-php/datastore-tools",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "knot-lib/datastore": "~0.1",
    "knot-lib/kernel": "~0.1",
    "knot-lib/datastore-services": "~0.1",
    "knot-php/module-knot-datastore-services": "~0.1",
    "knot-php/command": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "KnotPhp\\DataStore\\Tools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KnotPhp\\DataStore\\Tools\\Test\\": "test/include/",
      "KnotPhp\\DataStore\\Tools\\Demo\\": "demo/include/"
    }
  }
}
